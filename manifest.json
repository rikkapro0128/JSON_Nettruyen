{
  "name": "JSON Nettruyen",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab"],
  "icons": {
    "16": "/icon/anime_16.png",
    "32": "/icon/anime_32.png",
    "64": "/icon/anime_64.png",
    "128": "/icon/anime_128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*.nettruyengo.com/*"],
      "css": ["styles.css"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["socket.io.min.js"],
      "matches": ["http://*.nettruyengo.com/*"]
    }
  ]
}
